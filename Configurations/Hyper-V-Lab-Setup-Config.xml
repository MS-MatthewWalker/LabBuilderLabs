<?xml version="1.0" encoding="utf-8"?>
<labbuilderconfig name="WS2016_Combo_Workshop" version="1.0">
  <description>Lab Buildout for Windows Server 2016 MSInternal Workshop</description>
  <settings domainname="Corp.Contoso.COM" vhdparentpath=".\Virtual Hard Disk Templates" dsclibrarypath=".\LabDSCResources\" resourcepath=".\Resources\" LabId="Lab-" modulepath=".\Modules" />
  <resources>
    <msu name="WMF5.0-WS2012R2-W81" url="https://download.microsoft.com/download/2/C/6/2C6E1B4A-EBE5-48A6-B225-2D2058A9CEFB/Win8.1AndW2K12R2-KB3134758-x64.msu" />
    <msu name="windows10.0-kb4013418-x64_b7165b95be791aeb025135efe60e00db2800c5c6.msu" url="ResFolder/windows10.0-kb4013418-x64_b7165b95be791aeb025135efe60e00db2800c5c6.msu" />
    <msu name="windows10.0-kb4013429-x64_ddc8596f88577ab739cade1d365956a74598e710.msu" url="ResFolder/windows10.0-kb4013429-x64_ddc8596f88577ab739cade1d365956a74598e710.msu"/>
  </resources>
  <switches>
    <switch name="Public1" type="Private" />
    <switch name="SMB" type="Private" />
    <switch name="iSCSI" type="Private" />
    <switch name="LiveMigration" type="Private" />
    <switch name="Cluster" type="Private" />
  </switches>
  <templatevhds isopath="ISOFiles" vhdpath="VHDFiles" prefix="Template-">
    <templatevhd name="Windows Server 2016 Datacenter FULL" iso="14393.0.160715-1616.RS1_RELEASE_SERVER_EVAL_X64FRE_EN-US.ISO" url="https://www.microsoft.com/en-us/evalcenter/evaluate-windows-server-2016" vhd="Windows Server 2016 Datacenter Full.vhdx" Edition="Windows Server 2016 SERVERDATACENTER" OSType="Server" packages="windows10.0-kb4013418-x64_b7165b95be791aeb025135efe60e00db2800c5c6.msu,windows10.0-kb4013429-x64_ddc8596f88577ab739cade1d365956a74598e710.msu" features="" VHDFormat="VHDx" VHDType="Dynamic" Generation="2" vhdsize="40GB" />
    <templatevhd name="Windows Server 2016 Datacenter CORE" iso="14393.0.160715-1616.RS1_RELEASE_SERVER_EVAL_X64FRE_EN-US.ISO" url="https://www.microsoft.com/en-us/evalcenter/evaluate-windows-server-2016" vhd="Windows Server 2016 Datacenter Core.vhdx" edition="Windows Server 2016 SERVERDATACENTERCORE" OSType="Server" features="" packages="windows10.0-kb4013418-x64_b7165b95be791aeb025135efe60e00db2800c5c6.msu,windows10.0-kb4013429-x64_ddc8596f88577ab739cade1d365956a74598e710.msu" vhdformat="VHDx" vhdtype="Dynamic" generation="2" vhdsize="25GB" />
    <templatevhd name="Nano Server 2016" iso="14393.0.160715-1616.RS1_RELEASE_SERVER_EVAL_X64FRE_EN-US.ISO" url="https://www.microsoft.com/en-us/evalcenter/evaluate-windows-server-2016" vhd="Nano Server 2016.vhdx" edition="Windows Server 2016 SERVERDATACENTERNANO" ostype="Nano" packages="Microsoft-NanoServer-Guest-Package.cab,Microsoft-NanoServer-Defender-Package.cab,Microsoft-NanoServer-DSC-Package.cab,windows10.0-kb4013418-x64_b7165b95be791aeb025135efe60e00db2800c5c6.msu,windows10.0-kb4013429-x64_ddc8596f88577ab739cade1d365956a74598e710.msu" vhdformat="VHDx" vhdtype="Dynamic" generation="2" vhdsize="25GB" />
    <templatevhd name="Windows Server 2012 R2 Datacenter Core" iso="en_Windows_Server_2012_R2_with_update_x64_dvd_PATCHED.iso" vhd="Windows Server 2012 R2 Datacenter Core.vhdx" edition="2012R2CorePatched" ostype="Server" packages="WMF5.0-WS2012R2-W81" vhdformat="VHDx" vhdtype="Dynamic" generation="2" vhdsize="25GB" />
    <templatevhd name="Windows Server 2012 R2 Datacenter Full" iso="en_Windows_Server_2012_R2_with_update_x64_dvd_PATCHED.iso" url="https://www.microsoft.com/en-us/evalcenter/evaluate-windows-server-2012-r2" vhd="Windows Server 2012 R2 Datacenter Full.vhdx" edition="2012R2FullPatched" ostype="Server" packages="WMF5.0-WS2012R2-W81" vhdformat="VHDx" vhdtype="Dynamic" generation="2" vhdsize="40GB" />
    <templatevhd name="Windows Server 2012 R2 Datacenter Core Gen1" iso="en_Windows_Server_2012_R2_with_update_x64_dvd_PATCHED.iso" vhd="Windows Server 2012 R2 Datacenter Core Gen1.vhdx" edition="2012R2CorePatched" ostype="Server" packages="WMF5.0-WS2012R2-W81" vhdformat="VHDx" vhdtype="Dynamic" generation="1" vhdsize="25GB" />
  </templatevhds>
  <templates>
    <template name="Windows Server 2016 Datacenter FULL" vhd="Windows Server 2016 Datacenter Full.vhdx" sourcevhd=".\VHDFiles\Template-Windows Server 2016 Datacenter Full.vhdx" memorystartupbytes="1GB" processorcount="1" generation="2" version="8.0" administratorpassword="LS1setup!" timezone="Pacific Standard Time" ostype="Server" />
    <template name="Windows Server 2016 Datacenter CORE" vhd="Windows Server 2016 Datacenter Core.vhdx" sourcevhd=".\VHDFiles\Template-Windows Server 2016 Datacenter Core.vhdx" memorystartupbytes="1GB" processorcount="1" generation="2" version="8.0" administratorpassword="LS1setup!" timezone="Pacific Standard Time" ostype="Server" />
    <template name="Nano Server 2016" memorystartupbytes="2GB" processorcount="1" administratorpassword="LS1setup!" timezone="Pacific Standard Time" generation="2" version="8.0" ostype="Nano" vhd="Nano Server 2016.vhdx" sourcevhd=".\VHDFiles\Template-Nano Server 2016.vhdx" />
    <template name="2012 R2 Full" memorystartupbytes="2GB" processorcount="1" administratorpassword="LS1setup!" timezone="Pacific Standard Time" generation="2" version="5.0" ostype="Server" vhd="Windows Server 2012 R2 Datacenter Full.vhdx" sourcevhd=".\VHDFiles\Template-Windows Server 2012 R2 Datacenter Full.vhdx" />
    <template name="2012 R2 Core" memorystartupbytes="2GB" processorcount="1" administratorpassword="LS1setup!" timezone="Pacific Standard Time" generation="2" version="5.0" ostype="Server" vhd="Windows Server 2012 R2 Datacenter Core.vhdx" sourcevhd=".\VHDFiles\Template-Windows Server 2012 R2 Datacenter Core.vhdx" />
    <template name="2012 R2 Core Gen1" memorystartupbytes="2GB" processorcount="1" administratorpassword="LS1setup!" timezone="Pacific Standard Time" generation="1" version="5.0" ostype="Server" vhd="Windows Server 2012 R2 Datacenter Core Gen1.vhdx" sourcevhd=".\VHDFiles\Template-Windows Server 2012 R2 Datacenter Core Gen1.vhdx" />
  </templates>
  <vms>
    <vm name="Virt-Node1" template="Windows Server 2016 Datacenter FULL" computername="Virt-Node1" usedifferencingbootdisk="Y" ExposeVirtualizationExtensions="Y" processorcount="2" memorystartupbytes="4GB" DynamicMemoryEnabled="Y">
      <dsc configname="LAB_MEMBER_SERVER" configfile="LAB_MEMBER_SERVER.DSC.ps1">
        <parameters>
          DomainName = "Corp.Contoso.COM"
          DomainAdminPassword = "LS1setup!"
          PSDscAllowDomainUser = $True
          
          ServerName = 'DC'
          ServerTargetName = 'dc-virtcluster-target'
          TargetPortalAddress = '172.17.0.1'
          InitiatorPortalAddress = '172.17.0.11'

          Disks = @(
          @{ Name = 'CSVData';
          Number = '1';
          DriveLetter = 'T';
          FSLabel = 'CSVData';          
          },
          @{ Name = 'Witness';
          Number = '2';
          DriveLetter = 'W';
          FSLabel = 'Witness';          
          }
          )

          
        </parameters>
      </dsc>
      <adapters>
        <adapter name="Public1" switchname="Public1" MACAddressSpoofing="On">
          <ipv4 address="10.0.0.11" defaultgateway="10.0.0.255" PrefixLength="24" dnsserver="10.0.0.1" />
        </adapter>
        <adapter name="Public2" switchname="Public1" MACAddressSpoofing="On">
        </adapter>
        <adapter name="SMB" switchname="SMB" MACAddressSpoofing="On">
          <ipv4 address="192.168.0.11" PrefixLength="24" />
        </adapter>
        <adapter name="LiveMigration" switchname="LiveMigration" MACAddressSpoofing="On">
          <ipv4 address="172.18.0.11" PrefixLength="24" />
        </adapter>
        <adapter name="Cluster" switchname="Cluster" MACAddressSpoofing="On">
          <ipv4 address="172.16.0.11" PrefixLength="24" />
        </adapter>
        <adapter name="iSCSI" switchname="iSCSI" MACAddressSpoofing="On">
          <ipv4 address="172.17.0.11" PrefixLength="24" />
        </adapter>
      </adapters>
    </vm>
    <vm name="Virt-Node2" template="Windows Server 2016 Datacenter FULL" computername="Virt-Node2" usedifferencingbootdisk="Y" ExposeVirtualizationExtensions="Y" processorCount="2" memorystartupbytes="4GB" DynamicMemoryEnabled="Y">
      <dsc configname="LAB_MEMBER_SERVER" configfile="LAB_MEMBER_SERVER.DSC.ps1">
        <parameters>
          DomainName = "Corp.Contoso.COM"
          DomainAdminPassword = "LS1setup!"
          PSDscAllowDomainUser = $True
          
          ServerName = 'DC'
          ServerTargetName = 'dc-virtcluster-target'
          TargetPortalAddress = '172.17.0.1'
          InitiatorPortalAddress = '172.17.0.12'
          
        </parameters>
      </dsc>
      <adapters>
        <adapter name="Public1" switchname="Public1" MACAddressSpoofing="On">
          <ipv4 address="10.0.0.12" defaultgateway="10.0.0.255" PrefixLength="24" dnsserver="10.0.0.1" />
        </adapter>
        <adapter name="Public2" switchname="Public1" MACAddressSpoofing="On">
        </adapter>
        <adapter name="SMB" switchname="SMB" MACAddressSpoofing="On">
          <ipv4 address="192.168.0.12" PrefixLength="24" />
        </adapter>
        <adapter name="LiveMigration" switchname="LiveMigration" MACAddressSpoofing="On">
          <ipv4 address="172.18.0.12" PrefixLength="24" />
        </adapter>
        <adapter name="Cluster" switchname="Cluster" MACAddressSpoofing="On">
          <ipv4 address="172.16.0.12" PrefixLength="24" />
        </adapter>
        <adapter name="iSCSI" switchname="iSCSI" MACAddressSpoofing="On">
          <ipv4 address="172.17.0.12" PrefixLength="24" />
        </adapter>
      </adapters>
    </vm>
    <vm name="SOFS-Node1" template="2012 R2 Full" computername="SOFS-Node1" usedifferencingbootdisk="Y" processorcount="2" memorystartupbytes="2GB" DynamicMemoryEnabled="Y" Version="5.0" ExposeVirtualizationExtensions="N">
      <dsc configname="LAB_MEMBER_FAILOVERCLUSTER" configfile="LAB_MEMBER_FAILOVERCLUSTER.DSC.ps1">
        <parameters>
          DomainName = "Corp.Contoso.COM"
          DomainAdminPassword = "LS1setup!"
          PSDscAllowDomainUser = $True

          ServerName = 'DC'
          ServerTargetName = 'dc-sofscluster-target'
          TargetPortalAddress = '172.17.0.1'
          InitiatorPortalAddress = '172.17.0.21'

          Disks = @(
          @{ Name = 'CSVData';
          Number = '1';
          DriveLetter = 'T';
          FSLabel = 'CSVData';          
          },
          @{ Name = 'Witness';
          Number = '2';
          DriveLetter = 'W';
          FSLabel = 'Witness';          
          }
          )

          ClusterName = 'SOFSCluster'
          ClusterIPAddress = '10.0.0.31'
          SOFSName = 'SOFSShare'


        </parameters>
      </dsc>
      <adapters>
        <adapter name="Public1" switchname="Public1" MACAddressSpoofing="On">
          <ipv4 address="10.0.0.21" defaultgateway="10.0.0.255" PrefixLength="24" dnsserver="10.0.0.1" />
        </adapter>
        <adapter name="Public2" switchname="Public1" MACAddressSpoofing="On">
        </adapter>
        <adapter name="SMB" switchname="SMB" MACAddressSpoofing="On">
          <ipv4 address="192.168.0.21" PrefixLength="24" />
        </adapter>
        <adapter name="Cluster" switchname="Cluster" MACAddressSpoofing="On">
          <ipv4 address="172.16.0.21" PrefixLength="24" />
        </adapter>
        <adapter name="iSCSI" switchname="iSCSI" MACAddressSpoofing="On">
          <ipv4 address="172.17.0.21" PrefixLength="24" />
        </adapter>
      </adapters>
    </vm>
    <vm name="SOFS-Node2" template="2012 R2 Full" computername="SOFS-Node2" usedifferencingbootdisk="Y" processorCount="2" memorystartupbytes="2GB" DynamicMemoryEnabled="Y" Version="5.0" ExposeVirtualizationExtensions="N">
      <dsc configname="LAB_MEMBER_FAILOVERCLUSTER" configfile="LAB_MEMBER_FAILOVERCLUSTER.DSC.ps1">
        <parameters>
        
          DomainName = "Corp.Contoso.COM"
          DomainAdminPassword = "LS1setup!"
          PSDscAllowDomainUser = $True

          ServerName = 'DC'
          ServerTargetName = 'dc-sofscluster-target'
          TargetPortalAddress = '172.17.0.1'
          InitiatorPortalAddress = '172.17.0.22'

          JoinClusterName = 'SOFSCluster'
          JoinClusterIPAddress = '10.0.0.31'
           
        </parameters>
      </dsc>
      <adapters>
        <adapter name="Public1" switchname="Public1" MACAddressSpoofing="On">
          <ipv4 address="10.0.0.22" defaultgateway="10.0.0.255" PrefixLength="24" dnsserver="10.0.0.1" />
        </adapter>
        <adapter name="Public2" switchname="Public1" MACAddressSpoofing="On">
        </adapter>
        <adapter name="SMB" switchname="SMB" MACAddressSpoofing="On">
          <ipv4 address="192.168.0.22" PrefixLength="24" />
        </adapter>
        <adapter name="Cluster" switchname="Cluster" MACAddressSpoofing="On">
          <ipv4 address="172.16.0.22" PrefixLength="24" />
        </adapter>
        <adapter name="iSCSI" switchname="iSCSI" MACAddressSpoofing="On">
          <ipv4 address="172.17.0.22" PrefixLength="24" />
        </adapter>
      </adapters>
    </vm>
  </vms>
</labbuilderconfig>